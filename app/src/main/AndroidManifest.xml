<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.worldworkssys.stressless">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.worldworkssys.stressless.activity.AddAssignmentsActivity"></activity>
        <activity android:name="com.worldworkssys.stressless.activity.CourseInfoActivity" />
        <activity android:name="com.worldworkssys.stressless.activity.IntroActivity" />
        <activity android:name="com.worldworkssys.stressless.activity.AddCoursesActivity" />
        <activity android:name="com.worldworkssys.stressless.activity.HomeActivity" />
        <activity android:name="com.worldworkssys.stressless.activity.DetailsActivity" />
        <activity android:name="com.worldworkssys.stressless.activity.SignUpActivity" />
        <activity android:name="com.worldworkssys.stressless.activity.LoginActivity" />
        <activity android:name="com.worldworkssys.stressless.activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>